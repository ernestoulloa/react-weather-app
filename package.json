    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "eslint --fix-dry-run",
      "git add"
    ],
    "*.+(json|css|md)": [
      "npx prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
